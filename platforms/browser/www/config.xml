<?xml version='1.0' encoding='utf-8'?>
<widget id="com.verblike.liker" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Liker</name>
    <description>
        Bring attention to your profile for free
    </description>
    <author email="verbalike@gmail.com" href="https://verblike.com">
        Verblike Team
    </author>
    <content src="src/popup/popup.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
